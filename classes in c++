#include<iostream>
#include<sstream>
using namespace std;
class student
{
    private:
    int age;
    string first_name;
    string last_name;
    int standard;
    
    public:
    
    void set_age(int a)
    {
        age = a;
    }
    int get_age()
    {
        return age;
    }
    
    void set_first_name(string b)
    {
        first_name=b;
    }
    string get_first_name()
    {
        return first_name;
    }
    
    void set_last_name(string c)
    {
        last_name= c;
    }
    string get_last_name()
    {
        return last_name;
    }
    
    void set_standard(int d)
    {
        standard = d;
    }
    int get_standard()
    {
        return standard;
    }
    
    string to_string()
    {
        stringstream ss;
        ss<<age<<","<<first_name<<","<<last_name<<","<<standard;
        return ss.str();
    }
    
};
int main()
{
    int age ,standard;
    string first_name,last_name;
    cin>>age>>first_name>>last_name>>standard;
    student obj;
    obj.set_age(age);
    obj.set_first_name(first_name);
    obj.set_last_name(last_name);
    obj.set_standard(standard);
    
    cout<<obj.get_age()<<endl;
    cout<<obj.get_last_name()<<","<<obj.get_first_name()<<endl;
    cout<<obj.get_standard()<<endl;
    cout<<endl;
    cout<<obj.to_string();
    return 0;
    
}
